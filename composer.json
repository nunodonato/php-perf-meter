{
    "name": "nunodonato/php-perf-meter",
    "description": "This is my package php-perf-meter",
    "keywords": [
        "nunodonato",
        "php-perf-meter"
    ],
    "homepage": "https://github.com/nunodonato/php-perf-meter",
    "license": "MIT",
    "authors": [
        {
            "name": "Nuno Donato",
            "email": "nunodonato@fastmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Nunodonato\\PhpPerfMeter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nunodonato\\PhpPerfMeter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
